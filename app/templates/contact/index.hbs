<div class="row">
  &nbsp;
</div>
<div class="row">
  <div class="col-sm-4 col-sm-offset-6 contact">
    <h1>Contact</h1>
    <form role="form col-sm-8 col-offset-1">
      <div class="form-group">
        <label for="exampleInputEmail1">Email Address</label>
        <input type="email" class="form-control" id="exampleInputEmail1" placeholder="Enter email">
      </div>
      <div class="form-group">
        <label for="exampleInputPassword1">Message</label>
        <textarea class="form-control" id="exampleInputPassword1" placeholder="What's up?"></textarea>
      </div>
      {{#link-to 'contact.submitted' class="btn btn-default"}}Send{{/link-to}}
    </form>
  </div>
</div>
</div>
